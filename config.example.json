{
  "db": "postgres://user:pass@hostname/database",
  "port": 3000,
  "ssl": {
    "port": 3001,
    "key": "path/to/file",
    "cert": "path/to/file"
  },
  "keys": ["session keys"],
  "ldap": {
    "url": "ldaps://ldap.domain.com",
    "searchBase": "ou=people,dc=company,dc=com",
    "searchFilter": "(uid={{username}})",
    "tlsOptions": {
      "rejectUnauthorized": false
    }
  }
}
